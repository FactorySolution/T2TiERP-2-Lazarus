<?xml version="1.0" encoding="utf-8"?>
<t2ti-orm-mapping>
  <class name="BancoVO" table="banco" >

    <id name="Id" column="ID">
      <generator class="sequence">
        <param name="sequence">banco</param>
      </generator>
    </id>

    <property name="Codigo" column="CODIGO" caption="Codigo" width="80" type="String" />
    <property name="Nome" column="NOME" caption="Nome" width="450" type="String" />
    <property name="Url" column="URL" caption="Url" width="450" type="String" />

  </class>
</t2ti-orm-mapping>
