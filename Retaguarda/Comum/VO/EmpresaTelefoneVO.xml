<?xml version="1.0" encoding="utf-8"?>
<t2ti-orm-mapping>
  <class name="EmpresaTelefoneVO" table="empresa_telefone" >

    <id name="Id" column="ID">
      <generator class="sequence">
        <param name="sequence">empresa_telefone</param>
      </generator>
    </id>

    <property name="IdEmpresa" column="ID_EMPRESA" caption="Id Empresa" width="80" type="Integer" />
    <property name="Tipo" column="TIPO" caption="Tipo" width="80" type="Integer" />
    <property name="Numero" column="NUMERO" caption="Numero" width="112" type="String" />
    <property name="Observacao" column="OBSERVACAO" caption="Observacao" width="450" type="Memo" />

  </class>
</t2ti-orm-mapping>
